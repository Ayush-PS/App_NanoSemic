{
  "expo": {
    "name": "NanoSemic",
    "slug": "NanoSemic",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "NanoSemic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSBluetoothPeripheralUsageDescription": "We use this to connect to Bluetooth devices",
        "NSLocationWhenInUseUsageDescription": "We use your location to enable Bluetooth connectivity"
      }
    },
    "android": {
      "permissions": [
        "BLUETOOTH",
        "BLUETOOTH_ADMIN",
        "ACCESS_FINE_LOCATION"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jsm.NanoSemic"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
     "@config-plugins/react-native-ble-plx"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "19920de7-cdb7-4bbd-9551-9a4f60acb01e"
      }
    }
  }
}
